{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "alias": {
          "Root": "./",
          "Constants": "./constants",
          "Components": "./components",
          "Styles": "./styles",
          "Core": "./core",
          "Utils": "./utils",
          "Pages": "./pages",
          "Redux": "./redux",
          "I18n": "./i18n",
          "Services": "./services",
          "AxiosInstance": "./configuration/axios-config.js"
        }
      },
      "@babel/plugin-syntax-dynamic-import"
    ]
  ],
  "presets": ["@babel/preset-env", "@babel/preset-react"]
}
